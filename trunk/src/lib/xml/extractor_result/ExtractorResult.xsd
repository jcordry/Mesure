<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:element name="ExtractorResult">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Metric" minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="Metric">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Unit" minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
			<xsd:attribute name="name" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="Unit">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Time" minOccurs="1" maxOccurs="1"/>
			</xsd:sequence>
			<xsd:attribute name="name" type="xsd:string"/>
			<xsd:attribute name="config" type="xsd:string"/>
			<xsd:attribute name="result" type="xsd:string"/>
			<xsd:attribute name="reference" type="xsd:string"/>
			<xsd:attribute name="script" type="xsd:string"/>
			<xsd:attribute name="benchedmode" type="xsd:string"/>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="Time">
		<xsd:complexType>
			<xsd:attribute name="avg" type="xsd:double"/>
			<xsd:attribute name="sampleSize" type="xsd:long"/>
			<xsd:attribute name="stdError" type="xsd:double"/>
			<xsd:attribute name="trust" type="xsd:double"/>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>
